    <div class="col-6">
        <table class="table  ">
  <thead>
    <tr>
      <th scope="col">#Patients</th>
      <th scope="col">Name</th>
      <th scope="col">ID</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody class="table-group-divider ">
    <% patients.forEach(function(data){%>
    <tr>
      <th scope="row"><%=Num++;%></th>
      <td><%=data.name%></td>
      <td><%=data._id%></td>
      <td>@<%=data.username%></td>
    </tr>
    <%});%>
        </tbody>
    </table>
</div>
<div class="col-6">
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#Input data</th>
